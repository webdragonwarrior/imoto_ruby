%ul.sidebar__list
  - Administrate::Namespace.new(namespace).resources.each do |resource|
    - if [:attribute_items, :customize_prices, :photographer_attributes, :zip_codes, :order_attributes].exclude?(resource)
      %li
        = link_to display_resource_name(resource), [namespace, resource], class: "sidebar__link sidebar__link--#{nav_link_state(resource)}"
